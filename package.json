{
	"name": "quantflow",
	"version": "2.0.0",
	"description": "Quantflow trading terminal SPA with realtime mocks",
	"main": "index.js",
	"scripts": {
		"start": "webpack serve --env port=3000",
		"start:dev": "npm start",
		"start:dev:server": "node ./json-server/index.js",
		"build": "npm run build:prod",
		"build:prod": "webpack --env mode=production apiUrl=https://quantflow-mock.trading",
		"build:dev": "webpack --env mode=development",
		"typecheck": "tsc --noEmit",
		"lint": "npm run lint:ts && npm run lint:scss",
		"lint:ts": "eslint \"**/*.{ts,tsx}\"",
		"lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --fix",
		"lint:scss": "npx stylelint \"**/*.scss\"",
		"lint:scss:fix": "npx stylelint \"**/*.scss\" --fix",
		"test": "npm run test:unit",
		"test:unit": "jest --config ./config/jest/jest.config.ts",
		"test:e2e": "cypress run",
		"test:e2e:open": "cypress open",
		"test:ui": "npx loki test",
		"test:ui:ok": "npx loki approve",
		"test:ui:ci": "loki --requireReference --reactUri file:./storybook-static",
		"test:ui:report": "npm run test:ui:json && npm run test:ui:html",
		"test:ui:json": "node scripts/generate-visual-json-report.js",
		"test:ui:html": "reg-cli --from .loki/report.json --report .loki/report.html",
		"storybook": "start-storybook -p 6006 -c ./config/storybook",
		"storybook:build": "build-storybook -c ./config/storybook",
		"prepare": "husky install",
		"generate:slice": "node ./scripts/createSlice/index.js"
	},
	"sideEffects": [
		"*.scss",
		"*.css",
		"*.module.scss",
		"*.module.css"
	],
	"engines": {
		"node": ">=20.0.0",
		"npm": ">=9.8.0"
	},
	"keywords": [
		"trading",
		"quantflow",
		"fintech"
	],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@headlessui/react": "^1.7.17",
		"@reduxjs/toolkit": "^2.2.1",
		"axios": "^1.6.7",
		"concurrently": "^8.2.2",
		"date-fns": "^3.6.0",
		"decimal.js": "^10.4.3",
		"i18next": "^23.7.16",
		"i18next-browser-languagedetector": "^7.1.0",
		"i18next-http-backend": "^2.5.1",
		"nanoid": "^5.0.6",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-hook-form": "^7.51.5",
		"react-i18next": "^13.3.1",
		"react-redux": "^9.1.0",
		"react-router-dom": "^6.22.3",
		"react-virtualized": "^9.22.3",
		"recharts": "^2.10.3",
		"workbox-window": "^7.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.25.2",
		"@babel/preset-env": "^7.25.3",
		"@babel/preset-react": "^7.24.7",
		"@babel/preset-typescript": "^7.24.7",
		"@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
		"@storybook/addon-actions": "^6.5.16",
		"@storybook/addon-essentials": "^6.5.16",
		"@storybook/addon-interactions": "^6.5.16",
		"@storybook/addon-links": "^6.5.16",
		"@storybook/builder-webpack5": "^6.5.16",
		"@storybook/manager-webpack5": "^6.5.16",
		"@storybook/react": "^6.5.16",
		"@svgr/webpack": "^8.1.0",
		"@testing-library/jest-dom": "^6.4.2",
		"@testing-library/react": "^13.4.0",
		"@types/jest": "^29.5.12",
		"@types/node": "^20.11.30",
		"@types/react": "^18.2.64",
		"@types/react-dom": "^18.2.21",
		"@types/react-virtualized": "^9.21.21",
		"@types/webpack": "^5.28.5",
		"@types/webpack-bundle-analyzer": "^4.7.0",
		"@types/webpack-dev-server": "^4.7.2",
		"@typescript-eslint/eslint-plugin": "^7.2.0",
		"@typescript-eslint/parser": "^7.2.0",
		"babel-loader": "^9.1.3",
		"babel-plugin-i18next-extract": "^0.9.0",
		"copy-webpack-plugin": "^11.0.0",
		"css-loader": "^7.1.1",
		"cypress": "^13.6.6",
		"eslint": "^8.57.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-plugin-i18next": "^6.1.1",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-jsx-a11y": "^6.8.0",
		"eslint-plugin-react": "^7.34.1",
		"eslint-plugin-react-hooks": "^4.6.0",
		"file-loader": "^6.2.0",
		"html-webpack-plugin": "^5.6.0",
		"husky": "^9.0.11",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^29.7.0",
		"json-server": "^1.0.0-beta.2",
		"loki": "^0.33.0",
		"mini-css-extract-plugin": "^2.8.1",
		"react-refresh": "^0.14.0",
		"reg-cli": "^0.17.6",
		"regenerator-runtime": "^0.14.1",
		"sass": "^1.72.0",
		"sass-loader": "^14.2.1",
		"style-loader": "^4.0.0",
		"stylelint": "^16.3.1",
		"stylelint-config-standard-scss": "^13.1.0",
		"ts-loader": "^9.5.1",
		"ts-node": "^10.9.2",
		"typescript": "^5.4.2",
		"webpack": "^5.90.3",
		"webpack-bundle-analyzer": "^4.10.1",
		"webpack-cli": "^5.1.4",
		"webpack-dev-server": "^4.15.1"
	},
	"peerDependencies": {
		"react": "^18.3.1",
		"react-dom": "^18.3.1"
	},
	"loki": {
		"configurations": {
			"chrome.laptop": {
				"target": "chrome.docker",
				"width": 1366,
				"height": 768
			},
			"chrome.iphone7": {
				"target": "chrome.docker",
				"preset": "iPhone 7"
			}
		}
	}
}
